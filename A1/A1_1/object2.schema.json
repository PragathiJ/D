{
	"$schema": "http://json-schema.org/draft-07/schema",
	"$id": "http://example.com/object2.json",
	"type": "object",
	"title": "Personal details",
	"properties": {
		"name": {
			"type": "string"
		},
		"nickname": {
			"type": "string"
		},
		"email": {
			"type": "string",
			"format": "email"
		},
		"lastknownlocation": {
			"type": "object",
			"properties": {
				"x": {
					"type": "number"
				},
				"y": {
					"type": "number"
				}
			}
		}
	},

	"required": ["name", "nickname", "email", "lastknownlocation"]
}
